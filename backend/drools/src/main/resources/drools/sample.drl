package gyqw.xiaobaitu.drools;

import java.util.Map;

        rule "old-lady"
            salience 1
            no-loop
            when
                $map:Map();
                eval($map.get("HOUSEHOLDFULLADDRESS_CITY") != $map.get("STOREADDRESS_CITY"));
                eval($map.get("GENDER") == "F");
                eval(((Integer)$map.get("AGE")) > 40);
            then
        end